{
    "name": "codica-studio/apple-signin",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/apple-signin",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "suggest": {
        "codicastudio/socialpersistance": "Automatic user resolution and persistence for any Socialite driver."
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "laravel/socialite": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^6.0",
        "orchestra/testbench-dusk": "^6.0",
        "orchestra/testbench": "^6.0",
        "predis/predis": "^1.1",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelSignInWithApple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelSignInWithApple\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\GeneaLabs\\LaravelSignInWithApple\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
